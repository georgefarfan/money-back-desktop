{
  "name": "money-back-desktop",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "BagOfCoins",
    "Electron",
    "Mongo"
  ],
  "author": "Jorge Farfan<deecon_daryl@gmail.com>",
  "license": "ISC",
  "build": {
    "appId": "bagOfCoins-v1.0",
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^1.11.0"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "ejs-electron": "^2.1.0",
    "firebase": "^7.17.2",
    "jquery": "^3.5.1",
    "mustache": "^4.0.1",
    "rxjs": "^6.6.2",
    "typescript": "^3.9.7"
  }
}
